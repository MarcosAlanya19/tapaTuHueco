{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@*": ["src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "moduleResolution": "bundler"
  },
  "include": [
    ".astro/types.d.ts",
    "src",
    "**/*.ts",
    "**/*.tsx",
    "**/*.astro"
  ],
  "exclude": ["dist", "node_modules"]
}
